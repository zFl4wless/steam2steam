APP_NAME=Steam2Steam
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://your-app.vercel.app
ASSET_URL=https://your-app.vercel.app

LOG_CHANNEL=stderr
LOG_LEVEL=debug

DB_CONNECTION=sqlite

SESSION_DRIVER=cookie
