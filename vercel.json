{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "outputDirectory": "public",
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/(favicon\\.ico|favicon\\.svg|apple-touch-icon\\.png|logo\\.svg|robots\\.txt)",
            "dest": "/$1"
        },
        {
            "src": "/(.*\\.(css|js|json|ico|png|jpg|jpeg|svg|woff|woff2|ttf|eot|map|txt))",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php",
            "headers": {
                "cache-control": "no-cache"
            }
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://steam2steam.vercel.app",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",

        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": "/tmp/database.sqlite"
    }
}
